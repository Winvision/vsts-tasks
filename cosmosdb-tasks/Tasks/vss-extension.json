{
    "manifestVersion": 1,
    "id": "winvisionbv-cosmosdb-tasks",
    "name": "Tasks for Cosmos DB",
    "publisher": "winvision-bv",
    "version" : "1.0.3",
    "public": true,
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "description": "A build and release extension containing Cosmos DB related tasks.",
    "categories": [
        "Build and release"
    ],
    "tags": [
        "cosmos db",
        "document db",
        "json"
    ],
    "icons": {
        "default": "images/extension-icon.png"
    },
    "content": {
        "details": {
            "path": "overview.md"
        }
    },
    "files": [
        {
            "path": "DataMigrationTool"
        },
        {
            "path": "CosmosDBCollectionManagement"
        }
    ],
    "branding": {
        "color": "rgb(202, 5, 56)",
        "theme": "light"
    },
    "links": {
        "home": {
            "uri": "https://github.com/Winvision/vsts-tasks"
        },
        "issues": {
            "uri": "https://github.com/Winvision/vsts-tasks/issues"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/Winvision/vsts-tasks"
    },
    "contributions": [
        {
            "id": "DataMigrationTool",
            "description": "Cosmos DB Data Migration Tool",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "DataMigrationTool"
            }
        },
        {
            "id": "CosmosDBCollectionManagement",
            "description": "Cosmos DB Collection Utilities",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "CosmosDBCollectionManagement"
            }
        }
    ]
}